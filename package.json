{
  "name": "iugu2-node",
  "version": "0.3.0",
  "description": "Another API wrapper for IUGU.com ",
  "main": "lib/index.js",
  "scripts": {
    "build": "rimraf lib && tsc",
    "lint": "tslint src/**/*.ts --format stylish --type-check --project tsconfig.json",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "test": "nyc mocha",
    "test:watch": "mocha -w --reporter min"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shirayukikitsune/iugu2-node.git"
  },
  "keywords": [
    "iugu",
    "payment",
    "api",
    "node"
  ],
  "author": "Bruno Ferreira <shirayuki@kitsune.com.br>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shirayukikitsune/iugu2-node/issues"
  },
  "nyc": {
    "require": [
      "ts-node/register"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "homepage": "https://github.com/shirayukikitsune/iugu2-node#readme",
  "devDependencies": {
    "@types/chai": "^4.0.3",
    "@types/chai-as-promised": "0.0.31",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.22",
    "@types/proxyquire": "^1.3.27",
    "@types/sinon": "^2.3.3",
    "@types/sinon-chai": "^2.7.28",
    "chai": "^4.1.1",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^2.13.1",
    "jsdoc": "^3.5.4",
    "minami": "^1.2.3",
    "mocha": "^3.5.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.1.0",
    "rimraf": "^2.6.1",
    "sinon": "^3.2.0",
    "sinon-chai": "^2.13.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.6.0",
    "typescript": "^2.4.2"
  },
  "dependencies": {},
  "typings": "lib/index.d.ts"
}
